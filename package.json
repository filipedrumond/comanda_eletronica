{
  "name": "comanda_eletronica",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prepare": "npm i json-server -g --save",
    "db": "concurrently --kill-others \"npm run db_cardapio\" \"npm run db_dinamico\" \"npm run db_status\" \"npm run db_pagamentos\" \"npm run db_admin\"",
    "db_cardapio": "json-server --watch db/db_cardapio.json --port 8015 --host localhost",
    "db_dinamico": "json-server --watch db/db_dinamico.json --port 8016 --host localhost",
    "db_status": "json-server --watch db/db_status.json --port 8017 --host localhost",
    "db_pagamentos": "json-server --watch db/db_pagamentos.json --port 8018 --host localhost",
    "db_admin": "json-server --watch db/db_admin.json --port 8019 --host localhost",
    "dev": "npm run development",
    "development": "node node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "start": "concurrently --kill-others \"npm run watch\" \"npm run db\"",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@filipedp/simple_dialog": "^1.2.10",
    "bootstrap": "^4.3.1",
    "css-loader": "^3.2.0",
    "font-awesome": "^4.7.0",
    "jquery-mask-plugin": "^1.14.16",
    "js-md5": "^0.7.3",
    "json-server": "^0.15.0",
    "laravel-mix": "^4.1.2",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.22.10",
    "sass-loader": "^7.3.1",
    "vue": "^2.6.10",
    "vue-cookies": "^1.5.13",
    "vue-loader": "^15.7.1",
    "vue-resource": "^1.5.1",
    "vue-router": "^3.1.2",
    "vue-session": "^1.0.0",
    "vue-sessionstorage": "^1.0.0",
    "vue-style-loader": "^4.1.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.2",
    "vue-template-compiler": "^2.6.10"
  }
}